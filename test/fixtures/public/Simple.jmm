import io;
class Simple {

    public int add(int a, int b){
        int c;
        int k;
        int l;
        boolean f;
        boolean i;
        i = false;
        k = 3;
        l = 4;
        c = a - 2 * c + l;
        f = c < a - 2 * l && false;
        i =!(f && i);
        return c;
    }

    public static void main(String[] args){
        int a;
        int b;
        int c;
        Simple s;
        a = 20;
        b = 10;
        s = new Simple();
        c = s.add(a,b);
        io.println(c);
    }

    public int constInstr(){
        int c;
        c = 0;
        c = 4;
        c = 8;
        c = 14;
        c = 250;
        c = 400;
        c = 1000;
        c = 100474650;
		c = 10;
        return c;
    }
}



