import io;

/*
 * This class tests the compiler's capability of verifying
 * the different initializations of a variable.
 */
class DifferentLoads {
    public static void main(String[] args) {
        int a;
        a = 1;
        a = 10;
        a = 200;
        a = 10000;

        io.println(a);
    }
}